<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <name>Sierpinski Carpet Reverse</name>
    <id>org.inkscape.effect.sierpinskicarpetreverse</id>
    <param name="tab" type="notebook">
        <page name="Options" gui-text="Options">
            <param name="fractal_deep"     type="int"   min="0" max="10"  gui-text="Fractal deep:"     default="5"/>
            <param name="square_size"      type="float" min="1" max="500" gui-text="Square size (px):" default="100.0"/>
            <param name="color_hole"       type="color" gui-text="Color hole" default="#FF0000"/>
            <param name="color_background" type="color" gui-text="Color background" default="#000000"/>
        </page>
    </param>
    <effect>
        <object-type>all</object-type>
        <effects-menu>
            <submenu name="TrucomanX"/>
        </effects-menu>
    </effect>
    <script>
        <command location="inx" interpreter="python">sierpinski_carpet_reverse.py</command>
    </script>
</inkscape-extension>

